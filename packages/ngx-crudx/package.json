{
  "name": "ngx-crudx",
  "version": "1.0.1",
  "description": "A library for leveraging Repository pattern & Entity in angular apps at scale.",
  "author": "Akki Mahajan",
  "license": "MIT",
  "homepage": "https://github.com/androizer/ngx-crudx",
  "private": false,
  "scripts": {
    "prebuild": "rimraf dist/",
    "build": "ng-packagr -p package.json",
    "postbuild": "cpy \"../../README.md\" dist/",
    "clean": "rimraf node_modules",
    "test": "echo \"Write some tests dude!\" && exit 0",
    "semantic-release": "semantic-release"
  },
  "ngPackage": {
    "dest": "dist",
    "lib": {
      "entryFile": "src/public_api.ts"
    },
    "allowedNonPeerDependencies": [
      "lodash-es"
    ]
  },
  "peerDependencies": {
    "@angular/common": "^12.2.0",
    "@angular/core": "^12.2.0",
    "rxjs": "^6.6.0"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.5",
    "cpy-cli": "^3.1.1",
    "ng-packagr": "^12.2.0"
  },
  "dependencies": {
    "lodash-es": "^4.17.21",
    "tslib": "^2.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/androizer/ngx-crudx"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "release": {
    "branches": [
      "master"
    ]
  },
  "keywords": [
    "angular",
    "crudx",
    "repository",
    "entity",
    "dependency-injection"
  ]
}
